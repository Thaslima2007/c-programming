#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int findSquare(int n)
{
    return n * n;
}

int main()
{
    int num;
    scanf("%d", &num);
    if (num >= 1 && num <= 9) 
    {
        int square = findSquare(num);
        printf("Square of %d is %d\n", num, square);
    } else
    {
        printf("Invalid Input\n");
    }

    return 0;
}
