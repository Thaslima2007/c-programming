#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() 
{
    int n, original, sum = 0, digit;   
    scanf("%d", &n);
    if (n < 100 || n > 999) {
        printf("No\n");
        return 0;
    }
    original = n;
    while (n > 0) {
        digit = n % 10;
        sum += digit * digit * digit; 
        n /= 10;
    }
    if (sum == original)
        printf("Yes\n");
    else
        printf("No\n");

    return 0;
}

 
